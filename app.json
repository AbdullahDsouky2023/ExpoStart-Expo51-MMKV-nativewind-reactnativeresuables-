{
  "expo": {
    "name": "CustomerSurvay",
    "slug": "CustomerSurvay",
    "version": "1.0.0",
    "scheme": "CustomerSurvay",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abdullahdsouky3.CustomerSurvay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdullahdsouky3.CustomerSurvay"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5bf03c83-f548-4e5e-9364-2b2b1251f35c"
      }
    }
  },
  "runtimeVersion": {
    "policy": "appVersion"
  }
}
